<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ClassControllerGetAll</servlet-name>
  	<display-name>ClassControllerGetAll</display-name>
  	<description></description>
  	<servlet-class>com.demo.controller.ClassControllerGetAll</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertStudentController</servlet-name>
  	<display-name>InsertStudentController</display-name>
  	<description></description>
  	<servlet-class>com.demo.controller.student.InsertStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClassControllerGetAll</servlet-name>
  	<url-pattern>/ClassControllerGetAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertStudentController</servlet-name>
  	<url-pattern>/InsertStudentController</url-pattern>
  </servlet-mapping>
</web-app>
